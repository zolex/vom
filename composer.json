{
    "name": "zolex/vom",
    "description": "Versatile Object Mapper. Map arbitrary data to strictly typed models using PHP attributes.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Linden",
            "email": "zlx@gmx.de"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "symfony/property-access": "^7.3|^8.0",
        "symfony/serializer": "^7.3|^8.0",
        "symfony/type-info": "^7.3|^8.0",
        "phpdocumentor/reflection-docblock": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Zolex\\VOM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zolex\\VOM\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "doctrine/orm": "^3.0",
        "symfony/framework-bundle": "^7.3|^8.0",
        "symfony/yaml": "^7.3|^8.0"
    },
    "scripts": {
        "test": "tools/phpunit/vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zolex\\VOM\\Laravel\\Providers\\VersatileObjectMapperProvider"
            ]
        }
    }
}
