{
    "name": "zolex/vom",
    "description": "Versatile Object Mapper. Map arbitrary data to strictly typed models using PHP attributes.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Linden",
            "email": "zlx@gmx.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/property-access": "^6.4|^7.0",
        "symfony/serializer": "^6.4|^7.0",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Zolex\\VOM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zolex\\VOM\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpspec/prophecy-phpunit": "^2.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/orm-pack": "^2.4",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0",
        "friendsofphp/php-cs-fixer": "^3.49",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0"
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zolex\\VOM\\Laravel\\Proviers\\VersatileObjectMapperProvider"
            ]
        }
    }
}
