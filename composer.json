{
    "name": "zolex/vom",
    "description": "Versatile Object Mapper",
    "type": "symfony-bundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Andreas Linden",
            "email": "andreas.linden@coxautoinc.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/property-access": "^6.1|^7.0",
        "symfony/serializer": "^6.1|^7.0",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Zolex\\VOM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zolex\\VOM\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "modix/php-cs-rulesets": "^1.3",
        "phpspec/prophecy": "^1.17",
        "phpspec/prophecy-phpunit": "^2.0",
        "symfony/orm-pack": "^2.4",
        "symfony/phpunit-bridge": "^6.1|^7.0",
        "api-platform/core": "^3.2"
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit"
    }
}
